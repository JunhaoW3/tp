@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":DeleteMeetingNoteCommandParser" as DeleteMeetingNoteCommandParser LOGIC_COLOR
participant "deleteMeetingNote:DeleteMeetingNoteCommand" as DeleteMeetingNoteCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
participant "personToDeleteFrom:Person" as personToDeleteFrom MODEL_COLOR
participant "editedPerson:Person" as editedPerson MODEL_COLOR
end box

participant "lastShownList:List<Person>" as lastShownList #green

[-> LogicManager : execute\n("nDelete 1 1")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand\n("nDelete 1 1")
activate AddressBookParser

create DeleteMeetingNoteCommandParser
AddressBookParser -> DeleteMeetingNoteCommandParser
activate DeleteMeetingNoteCommandParser

DeleteMeetingNoteCommandParser --> AddressBookParser
deactivate DeleteMeetingNoteCommandParser

AddressBookParser -> DeleteMeetingNoteCommandParser : parse("1 1")
activate DeleteMeetingNoteCommandParser

create DeleteMeetingNoteCommand
DeleteMeetingNoteCommandParser -> DeleteMeetingNoteCommand
activate DeleteMeetingNoteCommand

DeleteMeetingNoteCommand --> DeleteMeetingNoteCommandParser : deleteMeetingNote
deactivate DeleteMeetingNoteCommand

DeleteMeetingNoteCommandParser --> AddressBookParser : deleteMeetingNote
deactivate DeleteMeetingNoteCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
DeleteMeetingNoteCommandParser -[hidden]-> AddressBookParser
destroy DeleteMeetingNoteCommandParser

AddressBookParser --> LogicManager : deleteMeetingNote
deactivate AddressBookParser

LogicManager -> DeleteMeetingNoteCommand : execute(m)
activate DeleteMeetingNoteCommand

DeleteMeetingNoteCommand -> Model : getFilteredPersonList()
activate Model

Model --> DeleteMeetingNoteCommand : lastShownList
deactivate Model

DeleteMeetingNoteCommand -> lastShownList : get(0)
activate lastShownList

lastShownList --> DeleteMeetingNoteCommand : personToDeleteFrom
deactivate lastShownList

DeleteMeetingNoteCommand -> personToDeleteFrom : getMeetingNotes()
activate personToDeleteFrom

personToDeleteFrom --> DeleteMeetingNoteCommand : meetingNotes

DeleteMeetingNoteCommand -> personToDeleteFrom : removeMeetingNote(meetingNoteToDelete)

create editedPerson
personToDeleteFrom -> editedPerson
activate editedPerson

editedPerson --> personToDeleteFrom : editedPerson
deactivate editedPerson

personToDeleteFrom --> DeleteMeetingNoteCommand : editedPerson
deactivate personToDeleteFrom


DeleteMeetingNoteCommand -> Model : setPerson(personToDeleteFrom, editedPerson)
activate Model

Model --> DeleteMeetingNoteCommand
deactivate Model

DeleteMeetingNoteCommand -> Model : setToActiveList();
activate Model

Model --> DeleteMeetingNoteCommand
deactivate Model

DeleteMeetingNoteCommand -> Model : refreshFilteredPersonList();
activate Model

Model --> DeleteMeetingNoteCommand
deactivate Model


create CommandResult
DeleteMeetingNoteCommand -> CommandResult
activate CommandResult

CommandResult --> DeleteMeetingNoteCommand : r
deactivate CommandResult

DeleteMeetingNoteCommand --> LogicManager : r
deactivate DeleteMeetingNoteCommand
destroy personToDeleteFrom

[<--LogicManager
deactivate LogicManager
@enduml
